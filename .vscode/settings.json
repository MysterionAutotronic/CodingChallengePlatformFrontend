{
    // cSpell
    "cSpell.language": "en,de-DE",
    "cSpell.words": [
        "amplimind",
        "april",
        "audi",
        "Avenir",
        "commonmark",
        "DDMMMMYYYY",
        "DDMYY",
        "december",
        "february",
        "january",
        "july",
        "june",
        "Kategorienwerte",
        "Markierungsabstand",
        "Markierungsgröße",
        "Markierungsstrich",
        "MDDYY",
        "milkdown",
        "Millis",
        "nord",
        "november",
        "october",
        "Paarmodus",
        "Parens",
        "Radiusachse",
        "Scrollschritt",
        "september",
        "Setfilter",
        "Strg",
        "STRG",
        "testid",
        "Thiscolumn",
        "toastify",
        "treemap",
        "ungroup",
        "Verknüpfungsoperator",
        "Verknüpfungsoperatoren",
        "Versätze",
        "zxcvbn",
        "Linhardt",
        "Hauser",
        "Hertrich"
    ],
    // SASS compiler
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": null,
            "savePathReplacementPairs": null,
            "linefeed": "lfcr",
            "indentType": "space",
            "indentWidth": 4
        }
    ],
    "liveSassCompile.settings.excludeList": [
        "/**/node_modules/**",
        "/.vscode/**",
        "/src/theme/**"
    ],
    "liveSassCompile.settings.generateMap": false,
    "i18n-ally.localesPaths": ["src/locales"],
    "i18n-ally.namespace": true,
    "i18n-ally.pathMatcher": "{locale}/{namespace}.json",
    "i18n-ally.keystyle": "nested",
    "i18n-ally.sourceLanguage": "en"
}
